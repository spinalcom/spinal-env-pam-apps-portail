{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "esnext",
    "module": "esnext",
    "strict": false,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "types": [
      "webpack-env",
      "cypress"
    ],
    "strictPropertyInitialization": false,
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "allowJs": true,
  },
    "include": [
        "./src/**/*",
        "./micro-apps/**/src/**/*", 
        "micro-apps/spinal-context-group/src/components/data-side/Modales/EditGroupModale.cy.ts",
        "./cypress/**/*",
      ]
}